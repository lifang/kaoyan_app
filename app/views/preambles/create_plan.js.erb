<%unless  @user_plan["TARGET_SCORE"].nil?%>
  $(".tab p").html("根据前测，您的理想分数最高是<%= @user_plan["TARGET_SCORE"].to_i %>,是否根据此分数生成复习计划");
  generate_flash_div(".tab");
  $(".tab #confirm").bind("click",function(){
  $(".fenshu_box").html("<%= escape_javascript(render :partial => "/preambles/personal_info",:object=>"#{@user_plan}".gsub("=>",":")) %>");
   $(".tab").css("display","none");
  });
<% else %>
  $(".fenshu_box").html("<%= escape_javascript(render :partial => "/preambles/personal_info",:object=>"#{@user_plan}".gsub("=>",":")) %>");
<% end %>

