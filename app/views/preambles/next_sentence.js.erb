<% unless @is_end %>
  $('.main').html("<%= escape_javascript(render :partial => 'sentence_div') %>");
  <% if @level_change_flag == true %>
    true_time = 0;
    error_time = 0;
  <% end %>
  arr_to_choice(_ans_arr);
  local_start_time = _ans_arr.length * 2;
  local_save_start();
<% else %>
  <% unless @category_id == Category::TYPE[:GRADUATE] %>
    window.location.href="/pretests/listen?category_id=<%= @category_id %>";
  <% else %>
    window.location.href="/preambles/test_result?category_id=<%= @category_id %>";
  <% end %>
<% end %>